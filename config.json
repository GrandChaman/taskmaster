{
	"prgm_list": [
		{
			"name": "Toto",
			"command": "cat /dev/random",
			"proc_nbr": 8,
			"start_at_launch": true,
			"should_restart": "always",
			"restart_retry_nbr": 5,
			"normal_exit_code": 0,
			"normal_run_time": 120,
			"stop_signal": "SIGPIPE",
			"stop_time": 20,
			"stdout_logfile": null,
			"stderr_logfile": "toto_err.txt",
			"begin_with_clean_env": true,
			"env": [
				{"TOTO": "Salut"},
				{"Bonjour": "Bonsoir"}
			],
			"cwd": "/tmp",
			"umask": "00222"
		}
	]
}
